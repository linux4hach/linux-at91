/dts-v1/;
#include "at91sam9g35.dtsi"
#include "at91sam9x5dm.dtsi"
#include "at91sam9x5ek.dtsi"

/ {
	model = "Atmel AT91SAM9G35-EK";
	compatible = "atmel,at91sam9g35ek", "atmel,at91sam9x5ek", "atmel,at91sam9x5", "atmel,at91sam9";

	ahb {
		apb {
			hlcdc: hlcdc@f8038000 {
				status = "okay";
			};

			macb0: ethernet@f802c000 {
				phy-mode = "rmii";
				status = "okay";
			};

			adc0: adc@f804c000 {
				status = "disabled";
			};

			pwm0: pwm@f8034000 {
				status = "okay";  
			}; 
 			rtc@fffffeb0 {
    				status = "okay";
  			};
    		};

   	};
 
	bl_reg: backlight_regulator {
		status = "okay";
	};

	panel_reg: panel_regulator {
		status = "okay";
	};

	backlight: backlight {
		status = "okay";
	};

	panel: panel {
		status = "okay";
	};


  gpio_keys {
    compatible = "gpio-keys";

    button_0 {
      debounce_interval = <50>;
      label = "button 0";
      gpios = <&pioC 16 GPIO_ACTIVE_LOW>;
      linux,code = <15>;
      gpio-key,wakeup;
    };
   button_1 {
      debounce_interval = <50>;
      label = "button 1";
      gpios = <&pioC 17 GPIO_ACTIVE_LOW>;
      linux,code = <16>;
      gpio-key,wakeup;
    };
    button_2 {
      debounce_interval = <50>;
      label = "button 2";
      gpios = <&pioC 18 GPIO_ACTIVE_LOW>;
      linux,code = <17>;
      gpio-key,wakeup;
    };

    button_3 {
      debounce_interval = <50>;
      label = "button 3";
      gpios = <&pioC 19 GPIO_ACTIVE_LOW>;
      linux,code = <19>;
      gpio-key,wakeup;
    };

    button_4 {
      debounce_interval = <50>;
      label = "button 4";
      gpios = <&pioC 20 GPIO_ACTIVE_LOW>;
      linux,code = <20>;
      gpio-key,wakeup;
    };
    button_5 {
      debounce_interval = <50>;
      label = "button 5";
      gpios = <&pioC 21 GPIO_ACTIVE_LOW>;
      linux,code = <21>;
      gpio-key,wakeup;
    };
    button_6 {
      debounce_interval = <50>;
      label = "button 6";
      gpios = <&pioC 22 GPIO_ACTIVE_LOW>;
      linux,code = <22>;
      gpio-key,wakeup;
    };
    button_7 {
      debounce_interval = <50>;
      label = "button 7";
      gpios = <&pioC 23 GPIO_ACTIVE_LOW>;
      linux,code = <24>;
      gpio-key,wakeup;
    };
    button_8 {
      debounce_interval = <50>;

      label = "button 8";
      gpios = <&pioC 25 GPIO_ACTIVE_LOW>;
      linux,code = <25>;
      gpio-key,wakeup;
    };


  };
  
     
};
